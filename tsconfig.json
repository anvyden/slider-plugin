{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "baseUrl": ".",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "exclude": ["node_modules"]
}
